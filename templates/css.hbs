@font-face {
	font-family: "{{fontName}}";
	font-display: block;
	src: {{{src}}};
}

{{baseSelector}} {
	font-family: {{fontName}};
	-moz-osx-font-smoothing: grayscale;
	-webkit-font-smoothing: antialiased;
    	font-weight: inherit;
	font-style: normal;
	font-variant: normal;
	line-height: 1;
	text-rendering: auto;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
